{
  "name": "n8n-nodes-azure-openai-responses",
  "version": "1.0.0",
  "description": "n8n community node for Azure OpenAI Response API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "azure",
    "openai",
    "responses",
    "ai"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mlangcode/n8n-nodes-azure-openai-responses",
  "author": {
    "name": "mlangcode"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlangcode/n8n-nodes-azure-openai-responses.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npx eslint -c .eslintrc.prepublish.js 'nodes/**/*.ts' 'credentials/**/*.ts' package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AzureOpenAI.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AzureOpenAIResponse/AzureOpenAIResponse.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.60.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {}
}

